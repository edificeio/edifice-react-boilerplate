{
  "name": "edifice-react-boilerplate",
  "version": "1.0.0-develop.20230881606",
  "description": "Edifice React boilerplate",
  "homepage": "https://github.com/opendigitaleducation/ode-react-boilerplate#readme",
  "bugs": {
    "url": "https://github.com/opendigitaleducation/ode-react-boilerplate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opendigitaleducation/ode-react-boilerplate.git"
  },
  "license": "GPL-3.0",
  "author": "Edifice",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "clean": "concurrently \"pnpm:clean:*\"",
    "clean:dist": "rm -rf dist",
    "clean:lighthouse": "rm -rf .lighthouseci",
    "clean:modules": "rm -rf node_modules",
    "clean:package": "rm -f pnpm-lock.yaml && rm -f package.json",
    "dev": "vite",
    "fix": "eslint --fix .",
    "format": "pnpm format:check && pnpm format:write",
    "format:check": "npx prettier --check .",
    "format:write": "npx prettier --write .",
    "preinstall": "npx only-allow pnpm",
    "lighthouse": "pnpm clean:lighthouse && lhci autorun",
    "lint": "eslint .",
    "pre-commit": "lint-staged",
    "prepare": "cd .. && husky install frontend/.husky",
    "preview": "vite preview"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "pnpm run format",
      "pnpm run fix"
    ]
  },
  "dependencies": {
    "@edifice-ui/icons": "develop",
    "@edifice-ui/react": "develop",
    "@react-spring/web": "9.7.3",
    "@tanstack/react-query": "5.8.4",
    "dayjs": "1.11.8",
    "i18next": "23.7.6",
    "i18next-http-backend": "2.4.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.11",
    "react-hook-form": "7.49.2",
    "react-i18next": "13.5.0",
    "react-intersection-observer": "9.4.4",
    "zustand": "4.3.8"
  },
  "devDependencies": {
    "@axe-core/react": "4.7.2",
    "@tanstack/react-query-devtools": "5.8.4",
    "@types/node": "20.2.5",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "@typescript-eslint/parser": "5.59.0",
    "@vitejs/plugin-react": "4.0.0",
    "clsx": "2.0.0",
    "concurrently": "8.1.0",
    "csstype": "3.1.2",
    "edifice-bootstrap": "develop",
    "edifice-ts-client": "develop",
    "eslint": "8.54.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.5",
    "husky": "8.0.3",
    "lint-staged": "15.1.0",
    "prettier": "3.1.0",
    "typescript": "5.2.2",
    "vite": "4.5.1",
    "vite-plugin-dts": "3.6.0",
    "vite-tsconfig-paths": "4.2.1"
  },
  "packageManager": "pnpm@8.6.6",
  "engines": {
    "node": "16 || 18"
  }
}