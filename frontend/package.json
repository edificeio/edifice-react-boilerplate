{
  "name": "boilerplate",
  "version": "0.0.0",
  "private": true,
  "license": "GPL-3.0",
  "author": "Edifice",
  "type": "module",
  "scripts": {
    "build": "pnpm dlx vite build",
    "dev": "nx serve",
    "pre-commit": "lint-staged",
    "prepare": "cd .. && husky frontend/.husky || true",
    "test": "nx test",
    "lint": "nx lint",
    "format": "pnpm format:check && pnpm format:write",
    "format:check": "npx prettier --check 'src/**/*.{tsx,ts}'",
    "format:write": "npx prettier --write 'src/**/*.{tsx,ts}'",
    "checkout": "npx @ccreusat/cli --checkout && rm -rf node_modules && rm -f pnpm-lock.yaml && pnpm install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "pnpm run format",
      "nx lint --fix"
    ]
  },
  "dependencies": {
    "@edifice-ui/icons": "latest",
    "@edifice-ui/react": "latest",
    "@tanstack/react-query": "5.59.0",
    "clsx": "2.1.1",
    "i18next": "23.15.1",
    "i18next-http-backend": "2.6.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "4.0.13",
    "react-hook-form": "7.53.0",
    "react-i18next": "15.0.2",
    "react-router-dom": "6.26.2",
    "zustand": "5.0.0-rc.2"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.0",
    "@nx/eslint": "19.8.3",
    "@nx/eslint-plugin": "19.8.3",
    "@nx/js": "19.8.3",
    "@nx/react": "19.8.3",
    "@nx/vite": "19.8.3",
    "@nx/web": "19.8.3",
    "@nx/workspace": "19.8.3",
    "@swc-node/register": "~1.10.9",
    "@swc/cli": "0.4.1-nightly.20240914",
    "@swc/core": "~1.7.26",
    "@swc/helpers": "~0.5.13",
    "@tanstack/react-query-devtools": "5.59.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "22.7.4",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "@vitejs/plugin-react": "^4.3.2",
    "@vitest/coverage-v8": "^2.1.2",
    "@vitest/ui": "^2.1.2",
    "edifice-bootstrap": "latest",
    "edifice-ts-client": "latest",
    "eslint": "~8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "2.30.0",
    "eslint-plugin-jsx-a11y": "6.10.0",
    "eslint-plugin-react": "7.37.1",
    "eslint-plugin-react-hooks": "4.6.2",
    "husky": "9.1.6",
    "jsdom": "~25.0.1",
    "lint-staged": "^15.2.10",
    "msw": "^2.4.9",
    "nx": "19.8.3",
    "prettier": "^3.3.3",
    "typescript": "~5.6.2",
    "vite": "^5.4.8",
    "vitest": "^2.1.2"
  },
  "nx": {
    "includedScripts": []
  },
  "packageManager": "pnpm@9.4.0",
  "engines": {
    "node": "18 || 20"
  },
  "efc": {
    "application": true,
    "prefixes": [
      "@edifice-ui",
      "edifice"
    ]
  }
}